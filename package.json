{
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf dist .next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint '**/*.{css,scss}'",
    "lint:style:fix": "stylelint '**/*.{css,scss}' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "pre-commit": "npx lint-staged",
    "commit": "git-cz",
    "serverless": "serverless",
    "serverless:build": "tsc -p tsconfig.serverless.json --skipLibCheck",
    "serverless:clean-ports": "npx kill-port 3001 3002 3003 3004",
    "serverless:clean": "rm -rf .serverless dist",
    "serverless:offline": "npm run serverless:clean && npm run serverless:build && serverless offline",
    "serverless:deploy": "serverless deploy",
    "serverless:check": "find .serverless -type f | sort",
    "serverless:dev": "serverless offline",
    "vercel-build": "next build",
    "test:ci": "jest --ci --passWithNoTests"
  },
