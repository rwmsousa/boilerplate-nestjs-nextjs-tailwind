{
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf dist .next",
    "build": "npm run clean && next build --no-lint && tsc -p tsconfig.server.json --skipLibCheck",
    "start": "NODE_ENV=production node dist/server/main.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint '**/*.{css,scss}'",
    "lint:style:fix": "stylelint '**/*.{css,scss}' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "pre-commit": "npx lint-staged",
    "commit": "git-cz",
    "serverless": "serverless",
    "serverless:build": "tsc -p tsconfig.serverless.json && cp -r .serverless/server/* .serverless/",
    "serverless:clean-ports": "npx kill-port 3001 3002 3003 3004",
    "serverless:clean": "rm -rf .serverless",
    "serverless:offline": "npm run serverless:clean && npm run serverless:build && ls -la .serverless && serverless offline --httpPort 3001 --lambdaPort 3003",
    "serverless:deploy": "serverless deploy",
    "serverless:check": "find .serverless -type f | sort"
  },
  "name": "boilerplate-vite-next-tailwind",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "aws-lambda": "^1.0.7",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tailwindcss/postcss": "^4.0.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "stylelint": "^16.17.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3"
