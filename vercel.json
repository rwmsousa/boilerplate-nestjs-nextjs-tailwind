{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "functions": {
    "src/server/serverless.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/serverless.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
